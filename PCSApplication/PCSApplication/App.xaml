<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PCSApplication.App">
	<Application.Resources>
        <ResourceDictionary>
            <x:String x:Key="profileIcon">profile.png</x:String>
            <x:String x:Key="LoginButtonIcon">resource://PCSApplication.Assets.password.svg</x:String>
            <x:String x:Key="ShowPasswordIcon">eye.png</x:String>
            <x:String x:Key="LogoBannerIcon">resource://PCSApplication.Assets.google.svg</x:String>
            <x:String x:Key="NavigationBarColor">#57579f</x:String>
            <Style x:Key="BottomNavBarStyle" TargetType="Label">
                <Setter Property="Text" Value="Dont Have an Account? Please register." />
                <Setter Property="FontSize" Value="Medium" />
                <Setter Property="BackgroundColor" Value="Transparent" />
                <Setter Property="TextColor" Value="WhiteSmoke"/>
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>